{
	"name": "got",
	"version": "9.6.0",
	"description": "Simplified HTTP requests",
	"license": "MIT",
	"repository": "sindresorhus/got",
	"main": "dist",
	"engines": {
		"node": ">=8.6"
	},
	"scripts": {
		"test": "npm run lint && npm run build && nyc ava --timeout=1m",
		"release": "np",
		"lint": "xo && tslint --format stylish --project .",
		"build": "del-cli dist && tsc",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"http",
		"https",
		"get",
		"got",
		"url",
		"uri",
		"request",
		"util",
		"utility",
		"simple",
		"curl",
		"wget",
		"fetch",
		"net",
		"network",
		"electron",
		"brotli"
	],
	"dependencies": {
		"@sindresorhus/is": "^0.14.0",
		"@szmarczak/http-timer": "^1.1.2",
		"cacheable-request": "^6.0.0",
		"decompress-response": "^4.0.0",
		"duplexer3": "^0.1.4",
		"get-stream": "^4.1.0",
		"lowercase-keys": "^1.0.1",
		"mimic-response": "^2.0.0",
		"p-cancelable": "^1.0.0",
		"to-readable-stream": "^1.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.2.0",
		"@types/node": "^10.12.18",
		"@typescript-eslint/eslint-plugin": "1.0.0",
		"ava": "^1.1.0",
		"coveralls": "^3.0.0",
		"del-cli": "^1.1.0",
		"delay": "^4.1.0",
		"eslint-config-xo-typescript": "0.4.1",
		"form-data": "^2.3.3",
		"get-port": "^4.0.0",
		"np": "^3.1.0",
		"nyc": "^13.1.0",
		"p-event": "^2.1.0",
		"pem": "^1.13.2",
		"proxyquire": "^2.0.1",
		"sinon": "^7.2.2",
		"slow-stream": "0.0.4",
		"tempfile": "^2.0.0",
		"tempy": "^0.2.1",
		"tough-cookie": "^3.0.0",
		"tslint": "^5.12.1",
		"tslint-xo": "^0.11.0",
		"typescript": "^3.2.2",
		"xo": "^0.24.0"
	},
	"browser": {
		"decompress-response": false,
		"electron": false
	},
	"ava": {
		"concurrency": 4
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"import/no-unresolved": "off",
			"@typescript-eslint/no-var-requires": "off",
			"@typescript-eslint/explicit-function-return-type": "off"
		}
	}
}
